											"physicalType": "String"
										},
										"sink": {
											"name": "UnitsSold",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "Unit Price",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "UnitPrice",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "Unit Cost",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "UnitCost",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
